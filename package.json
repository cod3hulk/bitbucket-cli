{
  "name": "bitbucket-cli",
  "description": "CLI support for bitbucket",
  "version": "0.0.1",
  "author": "Tomas AvÃ© @cod3hulk",
  "bin": {
    "bitbucket-cli": "./bin/run"
  },
  "bugs": "https://github.com/cod3hulk/bitbucket-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.3",
    "nodegit": "^0.23.0",
    "opn": "^5.4.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.3",
    "@oclif/test": "^1.2.1",
    "@oclif/tslint": "^3.1.0",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.5",
    "@types/nodegit": "^0.22.7",
    "@types/opn": "^5.1.0",
    "chai": "^4.2.0",
    "globby": "^8.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/cod3hulk/bitbucket-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bitbucket-cli",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "macos": {
      "identifier": "com.github.cod3hulk.bitbucket-cli"
    }
  },
  "repository": "cod3hulk/bitbucket-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
